{
  "name": "kira-encryptor-gui",
  "version": "0.6.0-beta",
  "description": "A GUI for Kira Encryptor",
  "main": "./out/main/index.mjs",
  "author": "JMMOLLER",
  "scripts": {
    "format": "prettier --write .",
    "lint": "eslint --cache .",
    "typecheck:node": "tsc --noEmit -p tsconfig.node.json --composite false",
    "typecheck:web": "tsc --noEmit -p tsconfig.web.json --composite false",
    "typecheck": "pnpm run typecheck:node && pnpm run typecheck:web",
    "start": "electron-vite preview",
    "dev": "electron-vite dev",
    "build:core-worker": "cd .. && pnpm run build:worker",
    "build": "pnpm run typecheck && electron-vite build",
    "postinstall": "electron-builder install-app-deps",
    "build:unpack": "pnpm run build && electron-builder --dir",
    "build:win": "pnpm run build && electron-builder --win && node scripts/generate-sha256.js",
    "build:mac": "electron-vite build && electron-builder --mac",
    "build:linux": "electron-vite build && electron-builder --linux"
  },
  "dependencies": {
    "7zip-bin": "^5.2.0",
    "@ant-design/icons": "^6.0.0",
    "@electron-toolkit/preload": "^3.0.1",
    "@electron-toolkit/utils": "^4.0.0",
    "@iconify-json/fluent-color": "^1.2.12",
    "@iconify/react": "^6.0.0",
    "@kira-encryptor/core": "file:..\\core\\kira-encryptor-core-0.6.0-beta.tgz",
    "@tailwindcss/vite": "^4.1.4",
    "antd": "^6.2.0",
    "bcryptjs": "^3.0.2",
    "electron-conf": "^1.3.0",
    "electron-updater": "^6.3.9",
    "moment": "^2.30.1",
    "node-7z": "^3.0.0",
    "tailwindcss": "^4.1.4",
    "tiny-uid": "^1.1.2"
  },
  "devDependencies": {
    "@electron-toolkit/eslint-config-prettier": "^3.0.0",
    "@electron-toolkit/eslint-config-ts": "^3.0.0",
    "@electron-toolkit/tsconfig": "^2.0.0",
    "@types/node": "^25.0.9",
    "@types/node-7z": "^2.1.10",
    "@types/react": "^19.1.1",
    "@types/react-dom": "^19.1.2",
    "@vitejs/plugin-react": "^5.1.2",
    "deep-object-diff": "^1.1.9",
    "electron": "^40.0.0",
    "electron-builder": "^26.4.0",
    "electron-vite": "^5.0.0",
    "eslint": "^9.24.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.19",
    "prettier": "^3.5.3",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "typescript": "^5.8.3",
    "vite": "^7.3.1",
    "vite-plugin-node-polyfills": "^0.25.0"
  }
}
